{
  "scripts": {
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview",
    "cspell": "npx cspell \"**/*.md\"",
    "format:check": "npx prettier --check \"**/*.md\"",
    "format:fix": "npx prettier --write \"**/*.md\"",
    "test:all" : "npm run format:check && npm run cspell && npm run docs:build"
  },
  "devDependencies": {
    "vitepress": "^1.6.3"
  },
  "dependencies": {
    "cspell": "^9.1.2",
    "leaflet": "^1.9.4",
    "prettier": "^3.6.2"
  }
}
